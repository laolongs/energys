        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:id="@+id/data_rate_ll"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/y122"
                    android:orientation="vertical">
                    <TextView
                        android:layout_height="match_parent"
                        android:layout_width="wrap_content"
                        android:paddingLeft="@dimen/x34"
                        android:gravity="center_vertical"
                        android:text="有功功率"
                        android:textSize="14sp"
                        android:textColor="@color/datatextcolor"/>
                </LinearLayout>
                <cn.tties.energy.chart.LineDataTwoChart
                    android:id="@+id/havakw_chart"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/y428">
                </cn.tties.energy.chart.LineDataTwoChart>
                <View
                    android:layout_below="@+id/data_ll"
                    style="@style/cut_off_line_5dp"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/y122"
                    android:orientation="vertical">
                    <TextView
                        android:layout_height="match_parent"
                        android:layout_width="wrap_content"
                        android:paddingLeft="@dimen/x34"
                        android:gravity="center_vertical"
                        android:text="无功功率"
                        android:textSize="14sp"
                        android:textColor="@color/datatextcolor"/>
                </LinearLayout>
                <cn.tties.energy.chart.LineDataChart
                    android:id="@+id/nokvar_chart"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/y428">
                </cn.tties.energy.chart.LineDataChart>
            </LinearLayout>
            <View
                android:layout_below="@+id/data_rate_ll"
                android:id="@+id/data_rate_vv"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/y20">
            </View>
            <LinearLayout
                android:layout_below="@+id/data_rate_vv"
                android:id="@+id/data_rate_bottom"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y98"
                android:layout_alignParentBottom="true"
                android:orientation="horizontal"
                android:background="@color/slategrey"
                >
                <LinearLayout
                    android:id="@+id/data_rate_time"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_weight="1">
                    <TextView
                        android:textSize="14sp"
                        android:id="@+id/data_rate_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/white"
                        android:text="选择时间"/>
                    <ImageView
                        android:src="@mipmap/arrows"
                        android:layout_marginLeft="@dimen/x23"
                        android:layout_width="@dimen/x14"
                        android:layout_height="@dimen/y24"
                        />
                </LinearLayout>
                <View
                    android:layout_width="@dimen/x2"
                    android:layout_height="match_parent"
                    android:background="@color/white"/>
                <LinearLayout
                    android:id="@+id/data_rate_electrical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_weight="1">
                    <TextView
                        android:textSize="14sp"
                        android:id="@+id/data_rate_ele_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:textColor="@color/white"
                        android:text="总电量"/>
                    <ImageView
                        android:src="@mipmap/arrows"
                        android:layout_marginLeft="@dimen/x23"
                        android:layout_width="@dimen/x14"
                        android:layout_height="@dimen/y24"
                        />
                </LinearLayout>
            </LinearLayout>
